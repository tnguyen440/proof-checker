{
  "agreementName": "SimpleContract",
  "elements": [
    {
      "elementName": "startDate",
      "elementType": "date",
      "elementRules": [
        {
          "ruleType": "constraint",
          "ruleText": "<VALUE> >= 01/01/2018"
        },
        {
          "ruleType": "constraint",
          "ruleText": "<VALUE> <= 31/12/2018"
        }    

        ]
    },
    {
      "elementName": "contractDays",
      "elementType": "integer",
      "elementRules": [
        {
          "ruleType": "constraint",
          "ruleText": "<VALUE> == 7 OR <VALUE> == 30"
        }
      ]
    },
    {
      "elementName": "endDate",
      "elementType": "integer",
      "elementRules": [
        {
          "rule": "endDate = startDate + DAYS( ./contractDays)"
        }
      ]
    }
  ],
  "parties": [
    {
